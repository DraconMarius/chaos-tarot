{
  "name": "chaos-tarot",
  "version": "0.0.1",
  "private": true,
  "description": "ai generated tarot, with preset card frames, and logs",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "cloudinary": "^1.35.0",
    "dotenv": "^16.0.3",
    "framer": "^2.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "node server/server.js",
    "develop": "concurrently \"cd server && npm run watch\" \"cd client && npm start\"",
    "install": "cd server && npm i && cd ../client && npm i",
    "seed": "cd server && npm run seed",
    "build": "cd client && npm run build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "engines": {
    "node": "16.18.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DraconMarius/chaos-tarot.git"
  },
  "author": "Mari K. Ma",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DraconMarius/chaos-tarot/issues"
  },
  "homepage": "https://github.com/DraconMarius/chaos-tarot#readme"
}